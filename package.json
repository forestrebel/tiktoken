{
  "name": "tiktoken-mobile",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:demo": "jest -t 'demo flow'",
    "dev": "npm-run-all --parallel start validate:video",
    "dev:clean": "npm-run-all clean validate reset",
    "validate": "npm-run-all --parallel validate:*",
    "validate:video": "node scripts/validate-video.js",
    "validate:size": "node scripts/check-size.js",
    "build": "cd android && ./gradlew assembleRelease",
    "distribute": "./scripts/distribute.sh",
    "clean": "rimraf android/app/build",
    "reset": "watchman watch-del-all && npm start -- --reset-cache",
    "prepare": "npm run clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-firebase/app": "18.7.3",
    "@react-native-firebase/auth": "18.7.3",
    "@react-native-firebase/storage": "18.7.3",
    "@react-native-masked-view/masked-view": "0.3.0",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/stack": "6.3.20",
    "ffmpeg-kit-react-native": "6.0.2",
    "firebase": "10.7.1",
    "metro-react-native-babel-preset": "0.76.8",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-document-picker": "9.0.1",
    "react-native-fs": "2.20.0",
    "react-native-gesture-handler": "2.14.0",
    "react-native-reanimated": "3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "3.29.0",
    "react-native-video": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^12.3.5",
    "@react-native-community/cli-platform-android": "^12.3.5",
    "@react-native-community/cli-platform-ios": "^12.3.5",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.6.3",
    "jest-junit": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18"
  }
}
